{
  "name": "website",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://brunobiluca.github.io/web",
  "scripts": {
    "start": "react-scripts start",
    "start:watch": "powershell .\\run_local.ps1",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy": "gh-pages -d build"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx",
      "quiet": false
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^7.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.4",
    "remark-gfm": "^3.0.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "react-test-renderer": "^18.2.0"
  }
}
